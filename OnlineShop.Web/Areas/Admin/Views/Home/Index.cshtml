@using OnlineShop.Model.ViewModel.User
@model List<EditViewModel>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>جداول داده</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-left">
                        <li class="breadcrumb-item"><a href="#">خانه</a></li>
                        <li class="breadcrumb-item active">جداول داده</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">جدول داده 1</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="example2" class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>نام و نام خانوادگی</th>
                                    <th>سفارش </th>
                                    <th>آدرس</th>
                                    <th>قابلیت ها</th>

                                </tr>
                            </thead>
                            @foreach(var user in Model)
                            {
                                <tr>
                                    <td> @user.Id </td>
                                    <td> @user.FullName </td>
                                    <td> @user.Order </td>
                                    <td> @user.Address </td>
                                    <td> - </td>
                                </tr>
                            }
                               
                     
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>